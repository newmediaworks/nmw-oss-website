<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.newmediaworks</groupId><artifactId>nmw-oss-parent</artifactId><version>1.3.0-POST-SNAPSHOT</version>
		<relativePath>../parent/pom.xml</relativePath>
	</parent>

	<groupId>com.newmediaworks</groupId><artifactId>nmw-oss-website</artifactId><version>1.1.0-SNAPSHOT</version>
	<packaging>war</packaging>

	<properties>
		<javadoc.breadcrumbs><![CDATA[<a target="_top" href="https://oss.newmediaworks.com/">NMW OSS</a>]]></javadoc.breadcrumbs>
		<shortTitle />
		<google.analytics.trackingId>UA-177004-2</google.analytics.trackingId>
	</properties>

	<name>oss.newmediaworks.com</name>
	<url>https://oss.newmediaworks.com/</url>
	<description>The oss.newmediaworks.com website.</description>
	<inceptionYear>2019</inceptionYear>

	<scm>
		<connection>scm:git:git://github.com/newmediaworks/${project.artifactId}.git</connection>
		<developerConnection>scm:git:git@github.com:newmediaworks/${project.artifactId}.git</developerConnection>
		<url>https://github.com/newmediaworks/${project.artifactId}</url>
		<tag>HEAD</tag>
	</scm>

	<profiles>
		<!-- Features only enabled during development time -->
		<profile>
			<id>development</id>
			<dependencyManagement>
				<dependencies>
					<dependency>
						<groupId>com.semanticcms</groupId><artifactId>semanticcms-openfile-bom</artifactId><version>1.4.0-SNAPSHOT</version>
						<type>pom</type>
						<scope>import</scope>
					</dependency>
					<dependency>
						<groupId>com.semanticcms</groupId><artifactId>semanticcms-view-all</artifactId><version>1.6.0-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.semanticcms</groupId><artifactId>semanticcms-view-tree</artifactId><version>1.4.4-POST-SNAPSHOT</version>
					</dependency>
					<dependency>
						<groupId>com.semanticcms</groupId><artifactId>semanticcms-view-what-links-here</artifactId><version>1.4.4-POST-SNAPSHOT</version>
					</dependency>
				</dependencies>
			</dependencyManagement>
			<dependencies>
				<dependency>
					<groupId>com.semanticcms</groupId><artifactId>semanticcms-openfile-all</artifactId>
					<type>pom</type>
				</dependency>
				<dependency>
					<groupId>com.semanticcms</groupId><artifactId>semanticcms-view-all</artifactId>
				</dependency>
				<dependency>
					<groupId>com.semanticcms</groupId><artifactId>semanticcms-view-tree</artifactId>
				</dependency>
				<dependency>
					<groupId>com.semanticcms</groupId><artifactId>semanticcms-view-what-links-here</artifactId>
				</dependency>
			</dependencies>
		</profile>
		<profile>
			<id>release</id>
			<build>
				<pluginManagement>
					<plugins>
						<plugin>
							<groupId>org.apache.maven.plugins</groupId><artifactId>maven-war-plugin</artifactId>
							<configuration>
								<overlays>
									<overlay>
										<groupId>com.newmediaworks</groupId><artifactId>nmw-email-taglib</artifactId>
										<type>jar</type>
										<classifier>javadoc</classifier>
										<targetPath>email-taglib/apidocs</targetPath>
									</overlay>
									<overlay>
										<groupId>com.newmediaworks</groupId><artifactId>nmw-payment-taglib</artifactId>
										<type>jar</type>
										<classifier>javadoc</classifier>
										<targetPath>payment-taglib/apidocs</targetPath>
									</overlay>
								</overlays>
							</configuration>
						</plugin>
						<plugin>
							<groupId>org.apache.tomcat.maven</groupId><artifactId>tomcat7-maven-plugin</artifactId>
							<configuration>
								<charset>UTF-8</charset>
								<server>oss.newmediaworks.com</server>
								<path>/</path>
								<update>true</update>
								<url>${tomcat7-maven-plugin-oss.newmediaworks.com}</url>
							</configuration>
						</plugin>
					</plugins>
				</pluginManagement>
			</build>

			<dependencyManagement>
				<dependencies>
					<!-- Only include Google Analytics on production releases to not pollute with dev data -->
					<dependency>
						<groupId>com.semanticcms</groupId><artifactId>semanticcms-google-analytics</artifactId><version>1.1.0-SNAPSHOT</version>
					</dependency>
				</dependencies>
			</dependencyManagement>

			<dependencies>
				<!-- Only include Google Analytics on production releases to not pollute with dev data -->
				<dependency>
					<groupId>com.semanticcms</groupId><artifactId>semanticcms-google-analytics</artifactId>
				</dependency>
				<!-- Dependencies for javadocs -->
				<dependency>
					<groupId>com.newmediaworks</groupId><artifactId>nmw-email-taglib</artifactId><version>1.0.1-SNAPSHOT</version>
					<classifier>javadoc</classifier>
					<scope>provided</scope>
				</dependency>
				<dependency>
					<groupId>com.newmediaworks</groupId><artifactId>nmw-payment-taglib</artifactId><version>1.0.0-POST-SNAPSHOT</version>
					<classifier>javadoc</classifier>
					<scope>provided</scope>
				</dependency>
			</dependencies>
		</profile>
	</profiles>

